version_tuple = (sys.version_info[0], sys.version_info[1])
if version_tuple < (3, 7):
  print(f'Unsupported Python version: {sys.version:s}, version 3.7 or higher '
        f'required.')
  sys.exit(1)

# Change PYTHONPATH to include ${python_module_name} so that we can get the version.
sys.path.insert(0, '.')

import ${python_module_name}  # pylint: disable=wrong-import-position
